{
  "coc.preferences.formatOnType": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "typescriptreact", "json", "javascriptreact", "typescript.tsx", "css", "Markdown"],
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.useQuickfixForLocations": true,
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "suggest.enablePreview": true,
  "tsserver.enable": true,
  "prettier.disableSuccessMessage": true,
  "prettier.eslintIntegration": true,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact", "typescript.tsx"],
  "eslint.packageManager": "yarn",
  "eslint.autoFixOnSave": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "git.enableGlobalStatus": false,
  "git.changedSign.text": "│",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.addedSign.text": "│",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.removedSign.text": "│",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.text": "│",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.text": "│",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete"
}
